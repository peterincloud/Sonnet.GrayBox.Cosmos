<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Peter Lloyd Rufus</Authors>
    <Product>GrayBox</Product>
    <Company>Sonnet Cloud</Company>
    <Description>Cloud services for Azure Cosmos Database in relation with Sonnet GrayBox services.</Description>
    <AssemblyVersion>1.1.2.0</AssemblyVersion>
    <FileVersion>1.1.2.0</FileVersion>
    <Version>1.1.2.0</Version>
    <PackageTags></PackageTags>
    <ApplicationIcon />
    <Win32Resource />
    <Title>Sonnet.GrayBox.Cosmos.Db</Title>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Resources\**" />
    <EmbeddedResource Remove="Resources\**" />
    <None Remove="Resources\**" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Azure.Cosmos" Version="3.20.1" />
    <PackageReference Include="Microsoft.Azure.DocumentDB.Core" Version="2.15.0" />
    <PackageReference Include="Microsoft.Extensions.Logging.Abstractions" Version="3.1.17" />
  </ItemGroup>

</Project>
